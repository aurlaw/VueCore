{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "Sqlite": "Data Source=blog.db;Cache=Shared"
  },
  "Elsa": {
    "Server": {
      "BaseUrl": "https://localhost:5001/esla"
    },
    "Smtp": {
      "Host": "localhost",
      "Port": "2525",
      "DefaultSender": "noreply@acme.com"
    },
    "Registration": {
      "ExpirationTimeMin": 5
    },
    "DocumentManager": {
      "TagName": "DocumentManagement",
      "SearchIndex": "DocumentIndex"
    }
  }
}
