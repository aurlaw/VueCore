{
  "name": "vue-core",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p start:parcel start:dotnet",
    "start:parcel": "parcel watch ./ClientComponents/bundle.js --out-dir ./wwwroot/",
    "start:dotnet": "dotnet watch run",
    "build": "run-s build:parcel build:dotnet",
    "build:parcel": "parcel build ./ClientComponents/bundle.js --out-dir ./wwwroot/",
    "build:dotnet": "dotnet publish -c Release"
  },
  "dependencies": {
    "vue": "^2.6.11",
    "vue-hot-reload-api": "^2.3.4"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "parcel": "1.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.1",
    "vue-template-compiler": "^2.6.12"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
